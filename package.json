{
  "productName": "Addic7ed Bot",
  "description": "Drop your series to automatically get the correct subtitle from addic7ed.",
  "version": "0.1.1",
  "repository": "https://github.com/Jorand/addic7ed-bot",
  "author": "Jorand",
  "license": "MIT",
  "keywords": [
    "Addic7ed",
    "eztv",
    "subtitle"
  ],
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron ./app --enable-logging",
    "dev": "NODE_ENV='development' npm run start",
    "pack": "build --dir",
    "dist": "build",
    "release": "build"
  },
  "build": {
    "appId": "com.electron.addic7ed-bot",
    "mac": {
      "category": "public.app-category.developer-tools",
      "publish": ["github"]
    },
    "win": {
    }
  },
  "devDependencies": {
    "electron": "^1.4.15",
    "electron-builder": "^13.6.0",
    "electron-packager": "^8.5.1",
    "electron-rebuild": "^1.5.7"
  }
}
