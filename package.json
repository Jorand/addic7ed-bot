    "build": "build",
    "release": "build -p always"
  },
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-builder": "^19.49.0",
    "electron-packager": "^10.1.0",
    "electron-rebuild": "^1.6.0"
  },
  "dependencies": {}
}
